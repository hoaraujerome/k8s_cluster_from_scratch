apiVersion: v1
kind: Config
clusters:
- name: default
  cluster:
    server: https://localhost:6443
    insecure-skip-tls-verify: true
users:
- name: system:kube-controller-manager
  user:
    client-certificate: /var/run/kubernetes/kube-controller-manager.crt
    client-key: /var/run/kubernetes/kube-controller-manager.key
contexts:
- name: default
  context:
    cluster: default
    user: system:kube-controller-manager
current-context: default
